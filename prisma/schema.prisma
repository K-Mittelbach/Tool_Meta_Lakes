// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Lake {
  id              Int @id @default(autoincrement())
  name            String
  longitude       String
  latitude        String
  maxDepth        String
  surfaceArea     String
  surfaceLevel    String
  catchmentArea   String
  countries       String
  isReviewed      Boolean
  isEdited        Boolean

}

